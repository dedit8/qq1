#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules/@pnpm/exe/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules/@pnpm/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules/@pnpm/exe/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules/@pnpm/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/@pnpm+exe@8.10.5/node_modules:/mnt/g/wamp/www/1 angu/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../@pnpm/exe/pnpm.exe"   "$@"
exit $?
